<p id="notice"><%= notice %></p>

<h1>Taskattachments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taskattachments.each do |taskattachment| %>
      <tr>
        <td><%= link_to 'Show', taskattachment %></td>
        <td><%= link_to 'Edit', edit_taskattachment_path(taskattachment) %></td>
        <td><%= link_to 'Destroy', taskattachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taskattachment', new_taskattachment_path %>
